<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/btn_a"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        tools:context=".MainActivity">

    <include
            android:id="@+id/setting_header"
            layout="@layout/setting_header" />

    <Button
            android:id="@+id/bank_btn"
            android:layout_width="200dp"
            android:layout_height="46dp"
            android:text="금융"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_header"
            app:layout_constraintVertical_bias="0.646" />

    <Button
            android:id="@+id/remnant_btn"
            android:layout_width="200dp"
            android:layout_height="46dp"
            android:text="기타"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_header"
            app:layout_constraintVertical_bias="0.188" />

    <Button
            android:id="@+id/password_btn"
            android:layout_width="200dp"
            android:layout_height="46dp"
            android:text="비밀번호"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_header"
            app:layout_constraintVertical_bias="0.337" />

    <Button
            android:id="@+id/email_btn"
            android:layout_width="200dp"
            android:layout_height="46dp"
            android:text="이메일"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_header"
            app:layout_constraintVertical_bias="0.475" />

    <Button
            android:id="@+id/address_btn"
            android:layout_width="200dp"
            android:layout_height="46dp"
            android:text="주소"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/setting_header"
            app:layout_constraintVertical_bias="0.812" />


</androidx.constraintlayout.widget.ConstraintLayout>